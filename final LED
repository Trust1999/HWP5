#define F_CPU 1000000UL
#include <avr/io.h>
#include <util/delay.h>

int main (){

	DDRB |= (1<<1)|(1<<2);
	//DDRC = 0b00011111;
	DDRD |=(1<<7)|(1<<6)|(1<<5)|(1<<1)|(1);
	DDRC |=(1<<5)|(1<<4)|(1<<3)|(1<<2)|(1<<1)|(1);

	//OCR1A = 64;
	//OCR1B = 64;

	//PORTC = 0b00111111;
	//PORTD = 0b11100011;;
	//PORTC = 0b00011111;
	//PORTD = 0b00000011;
		

	while(1)
	{
			//PORTC = 0b00111111;
			PORTD |= (1<<7)|(1<<6)|(1<<5)|(1<<1)|(1);
			PORTB |= (0<<1)|(0<<2);
			PORTC |=(1<<5)|(1<<4)|(1<<3)|(1<<2)|(1<<1)|(1);
	}	

	return 0;
}
